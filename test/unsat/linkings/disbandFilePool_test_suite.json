[
    {
        "testing": "should test if a pool is properly disbanded",
        "initial": {
            "poolId": 1,
            "file1": "pool_1",
            "file2": "pool_1",
            "file3": "pool_1",
            "reverseLinkings": {
                "pool_1": ["file1", "file2", "file3"]
            }
        },
        "file_in_pool_to_disband": "file1",
        "expected": {
            "poolId": 1,
            "reverseLinkings": {}
        }
    },
    {
        "testing": "should test if a linkings file is unchanged when a nonexisting pool is disbanded",
        "initial": {
            "poolId": 1,
            "file1": "pool_1",
            "file2": "pool_1",
            "file3": "pool_1",
            "reverseLinkings": {
                "pool_1": ["file1", "file2", "file3"]
            }           
        },
        "file_in_pool_to_disband": "file4",
        "expected": {
            "poolId": 1,
            "file1": "pool_1",
            "file2": "pool_1",
            "file3": "pool_1",
            "reverseLinkings": {
                "pool_1": ["file1", "file2", "file3"]
            }
        }
    }
]